int solution(string s1, string s2) 
{
    vector<int> usedIndex;
    int quantity = 0;
    for(int i = 0; i < s1.size(); i++)
        for(int j = 0; j < s2.size(); j++)
            if (s1[i] == s2[j] && find(usedIndex.begin(), usedIndex.end(), j) == usedIndex.end())
            {
                usedIndex.push_back(j);
                quantity++;
                break;
            }
    return quantity;
}
